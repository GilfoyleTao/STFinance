html,
body,
ion-app {
  background-color: transparent !important;
}

.list-ios .item-block .item-inner {
  border-bottom: 0px solid #c8c7cc;
}

.list-md .item-block .item-inner {
  border-bottom: 0px solid #c8c7cc;
}

.item-ios {
  border-bottom: 0.55px solid #c8c7cc;
}

.item-md {
  border-bottom: 1px solid #c8c7cc;
}

// left-menu
.menu-banner {
  background: url('../assets/images/side-menu/banner.png');
  background-size: 100% 100%
}

.menu-icon-mission {
  width: 16px;
  height: 13px
}

.menu-icon-manufacture {
  width: 18px;
  height: 14px
}

.menu-height-36 {
  height: 36px
}

.menu-top-r25 {
  top: 25%;
}

.menu-top-r50 {
  top: 50%;
} // 安卓按钮居右
.bar-buttons-md {
  -webkit-order: 4;
  -ms-flex-order: 4;
  order: 1;
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
} // 行内垂直居中
.aq-vertical-center {
  display: flex;
  align-items: center !important;
  justify-content: center !important;
} // 行内水平 & 垂直居中
.aq-vertical-middle {
  display: flex !important;
  align-items: center !important;
} // flex 居右
.aq-flex-right {
  justify-content: flex-end !important;
  -webkit-justify-content: flex-end !important;
  -moz-justify-content: flex-end !important;
}

.aq-visible {
  visibility: visible !important;
} // 显示块元素
.aq-block {
  display: block !important;
} // 显示行内块元素
.aq-inline-block {
  display: inline-block;
} // 隐藏元素
.aq-none {
  display: none;
} // 相对定位
.aq-relative {
  position: relative;
} // 固定定位
.aq-fixed {
  position: fixed;
} // 绝对定位跟相对定位成对使用
.aq-absolute {
  position: absolute;
} // 手动增加滚动条
.aq-scroll {
  overflow: scroll;
} // 圆形
.aq-circle {
  border-radius: 50%;
} // 圆角 1~10
@for $i from 1 through 15 {
  .aq-radius-#{$i} {
    border-radius: 1px * $i !important;
  }
} // svg 图标和图片图标的宽和高 8, 12, 16, 20, 24, 28, 32 ... 100
@for $i from 4 through 100 {
  @if $i%4==0 {
    .aq-icon-#{$i} {
      width: 1px * $i !important;
      height: 1px * $i !important;
    }
  }
} // padding 上下左右 4 ~52
$padding-list: 'padding-top',
'padding-bottom',
'padding-left',
'padding-right';
@each $padding in $padding-list {
  @for $i from 0 through 52 {
    @if $i%4==0 {
      .aq-#{$padding}-#{$i} {
        #{$padding}: 1px * $i !important;
      }
    }
  }
} // margin 上下左右4~52
$margin-list: 'margin-top',
'margin-bottom',
'margin-left',
'margin-right';
@each $margin in $margin-list {
  @for $i from 0 through 52 {
    @if $i%4==0 {
      .aq-#{$margin}-#{$i} {
        #{$margin}: 1px * $i !important;
      }
    }
  }
} // 特号字体大小
.aq-text-size-xsmall {
  font-size: 1.0rem !important;
} // 小号字体大小
.aq-text-size-small {
  font-size: 1.2rem !important;
} // 正常字体大小，默认不需要设置
.aq-text-size-normal {
  font-size: 1.4rem !important;
} // 大号字体大小
.aq-text-size-big {
  font-size: 1.6rem !important;
} // 超大号字体大小
.aq-text-size-large {
  font-size: 1.8rem !important;
} // 最大号字体大小
.aq-text-size-xlarge {
  font-size: 3.0rem !important;
} // 超长显示省略号
.aq-text-ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
} // 默认border宽度
.aq-border {
  border: 1px solid !important;
} // 默认border top
.aq-border-top {
  border-top: 1px solid;
} // 默认border bottom
.aq-border-bottom {
  border-bottom: 1px solid;
} // 默认border left
.aq-border-left {
  border-left: 1px solid;
} // 默认border right
.aq-border-right {
  border-right: 1px solid;
}

@for $i from 1 through 4 {
  .aq-border-width-#{$i} {
    border-width: 1px * $i !important;
  }
