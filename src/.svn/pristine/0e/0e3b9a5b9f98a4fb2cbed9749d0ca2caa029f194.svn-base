$color in $colors {
  .aq-text-color-#{$colorName} {
    color: $color !important;
  }
  .aq-border-color-#{$colorName} {
    border-color: $color !important;
  }
  .aq-bg-color-#{$colorName} {
    background: $color !important;
    background-color: $color !important;
  }
} // 清除背景
.aq-background-none {
  background: none;
