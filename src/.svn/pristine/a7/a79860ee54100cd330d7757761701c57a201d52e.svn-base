Page-Register {
    .login-icon{
        height: 20px;
        width: 15px;
    }
    .item-ios [item-left], .item-ios [item-start],.item-md [item-left], .item-md [item-start] {
        margin: 0;
    }
    .item-ios.item-block .item-inner {
        padding-right: 8px;
        padding-right: 8px;
        border-bottom: 0px solid #c8c7cc;
        height: 16px !important;
    }
    .item-md.item-block .item-inner {
        padding-right: 8px;
        border-bottom: 0px solid #dedede;
        height: 16px !important;
    }
    .label-ios + ion-input .text-input, .label-ios + ion-textarea .text-input, .label-ios + .input + .cloned-input {
        margin-left: 0px;
        width: calc(100% - (16px / 2) - 16px);
    }
    .text-input-ios {
        margin: 0px;
        padding: 0;
        width: calc(100% - 8px);
    }
    .label-md + ion-input .text-input, .label-md + ion-textarea .text-input, .label-md + .input + .cloned-input {
        margin-left: 0px;
        width: calc(100% - (16px / 2) - 16px);
    }
    .text-input-md {
        margin: 0px;
        padding: 0;
        width: calc(100% - 8px);
    }
    .phone-number {
        -webkit-box-flex: 0;
        -webkit-flex: 0 0 20px;
        -ms-flex: 0 0 20px;
        flex: 0 0 20px;
        width: 20px;
        min-width: 20px;
        max-width: 200px;
    }
    .phone-password {
        -webkit-box-flex: 0;
        -webkit-flex: 0 0 110px;
        -ms-flex: 0 0 110px;
        flex: 0 0 110px;
        width: 110px;
        min-width: 110px;
        max-width: 110px ;
    }
    .height-35{
        height: 35px;
    }
}