        <!-- 真实姓名 -->
        <ion-row class="Message ">
            <ion-col col-3>
                <span class="aq-textsize-14 aq-textcolor-grey-60 User">姓名</span>
            </ion-col>
            <ion-col col-8>
                <ion-input type="text" class="aq-textsize-14 aq-textcolor-grey-50 Username" placeholder="请输入姓名"  [(ngModel)]="UserTrueName">{{GetUserInfoModel?.UserTrueName}}</ion-input>            </ion-col>
            <ion-col col-1 class="Arrows"></ion-col>
        </ion-row>
        <!-- 身份证码 -->
        <ion-row class="Message ">
            <ion-col col-3>
                <span class="aq-textsize-14 aq-textcolor-grey-60 User">身份证码</span>
            </ion-col>
            <ion-col col-8>
                <ion-input  class="aq-textsize-14 aq-textcolor-grey-50 Idcard" placeholder="请输入身份证码" [(ngModel)]="UserIdentityCode" maxlength="18">{{GetUserInfoModel?.UserIdentityCode}}</ion-input>
            </ion-col>
            <ion-col col-1 class="Arrows"></ion-col>
        </ion-row>
    </ion-grid>
