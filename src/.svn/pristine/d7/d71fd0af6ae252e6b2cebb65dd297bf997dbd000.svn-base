<ion-header>
    <ion-navbar color="MintCream">
        <ion-title>
            <span class="aq-title-LoginPwd">关于app</span>
        </ion-title>
    </ion-navbar>
</ion-header>
<ion-content>
    <ion-grid text-center>
        <ion-row>
            <ion-col>
                <!-- <img src="assets/img/login/logo.png" alt="" class="STPropertyLogo"><br> -->
                <img [src]="ImgUrl+_AboutAppModel?.LogoUrl" alt="" class="STPropertyLogo"><br>
            </ion-col>
        </ion-row>
    </ion-grid>
    <ion-grid >
        <ion-row>
            <ion-col>
                <p class="aq-textsize-16 aq-textcolor-grey-60">公司名称：&nbsp;{{_AboutAppModel?.AppTitle}}</p>
                <p class="aq-textsize-16 aq-textcolor-grey-60">拨打电话&nbsp;&nbsp;:&nbsp;&nbsp;<a href="tel:{{_AboutAppModel?.AppContactPhone}}">{{_AboutAppModel?.AppContactPhone}}</a></p>
            </ion-col>
        </ion-row>
    </ion-grid>
</ion-content>
<ion-footer>
    <ion-grid>
        <ion-row>
            <ion-col col-12 text-center>
            </ion-col>
        </ion-row>
    </ion-grid>
</ion-footer>